{
  "name": "image-reviewer-gui",
  "main": "public/electron/app.ts",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "build": {
    "extends": null,
    "productName": "ImageReviewer",
    "appId": "com.imageReviewer.app"
  },
  "scripts": {
    "start": "NODE_ENV=development concurrently -k 'BROWSER=none react-scripts start' 'npm:electron'",
    "electron": "wait-on tcp:3000 && electron .",
    "build": "yarn clean && electron-builder",
    "build:test": "yarn clean && electron-builder --dir",
    "clean": "rm -rf dist",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.2",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "framer-motion": "^7.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.4.0",
    "electron": "^20.1.4",
    "electron-builder": "^23.3.3",
    "node-sass": "^7.0.3",
    "wait-on": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
